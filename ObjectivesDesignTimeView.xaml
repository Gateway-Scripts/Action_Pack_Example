<UserControl x:Class="ObjectivesAP.ObjectivesDesignTimeView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ObjectivesAP"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800" Background="OliveDrab">
    <Grid>
        <StackPanel>
            <TextBlock Text="Select Objectives to print." FontWeight="Bold" Margin="10"/>
            <CheckBox Margin="10" Content="Optimization Objectives" 
                      IsChecked="{Binding ObjectivesChecked}"/>
            <CheckBox Margin="10" Content="Optimization Parameters" 
                      IsChecked="{Binding ParametersChecked}"/>
        </StackPanel>  
    </Grid>
</UserControl>
